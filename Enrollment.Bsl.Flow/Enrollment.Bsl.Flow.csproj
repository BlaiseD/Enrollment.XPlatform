<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <CopyLocalLockFileAssemblies>true</CopyLocalLockFileAssemblies>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Rulesets\logvalidationerrors.module" />
    <None Remove="Rulesets\logvalidationerrors.resources" />
    <None Remove="Rulesets\saveacademic.module" />
    <None Remove="Rulesets\saveacademic.resources" />
    <None Remove="Rulesets\validateacademic.module" />
    <None Remove="Rulesets\validateacademic.resources" />
    <None Remove="Rulesets\validateinstitutions.module" />
    <None Remove="Rulesets\validateinstitutions.resources" />
    <None Remove="Rulesets\VariablesMetadata.xml" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Rulesets\logvalidationerrors.module" />
    <EmbeddedResource Include="Rulesets\logvalidationerrors.resources" />
    <EmbeddedResource Include="Rulesets\saveacademic.module" />
    <EmbeddedResource Include="Rulesets\saveacademic.resources" />
    <EmbeddedResource Include="Rulesets\validateacademic.module" />
    <EmbeddedResource Include="Rulesets\validateacademic.resources" />
    <EmbeddedResource Include="Rulesets\validateinstitutions.module" />
    <EmbeddedResource Include="Rulesets\validateinstitutions.resources" />
    <EmbeddedResource Include="Rulesets\VariablesMetadata.xml" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="LogicBuilder.RulesDirector" Version="1.0.3" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Enrollment.Bsl.Business\Enrollment.Bsl.Business.csproj" />
    <ProjectReference Include="..\Enrollment.Bsl.Utils\Enrollment.Bsl.Utils.csproj" />
    <ProjectReference Include="..\Enrollment.Parameters\Enrollment.Parameters.csproj" />
    <ProjectReference Include="..\Enrollment.Repositories\Enrollment.Repositories.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>
